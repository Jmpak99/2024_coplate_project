<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/r5uv5m3nlrw6"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFFFF"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rzbof7mu9yh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="14dp"
                android:background="#FFFFFFFF"
                android:elevation="6dp"
                android:orientation="vertical"
                android:paddingBottom="14dp">

                <LinearLayout
                    android:id="@+id/r5xb0g0ikq7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginHorizontal="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/goinfo"
                        android:layout_width="20dp"
                        android:layout_height="18dp"
                        android:layout_marginEnd="15dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/left" />

                    <TextView
                        android:id="@+id/rlkup342fez"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="음식메뉴"
                        android:textColor="#000000"
                        android:textSize="20sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rtgvzcu7kj6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/r8pcirc628uv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/sb0000000sw1tl54bl54bff998bee"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingVertical="14dp">

                    <TextView
                        android:id="@+id/rxnxayq98foj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="면류"
                        android:textColor="#000000"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/rgx5zda6k8ya"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/sb0000000sw1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingVertical="14dp">

                    <TextView
                        android:id="@+id/rnis300gpwpn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="정식류"
                        android:textColor="#000000"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/rtyhnv9baagp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/sb0000000sw1tr66br66"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingVertical="14dp">

                    <TextView
                        android:id="@+id/r6973umycdoi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="튀김류"
                        android:textColor="#000000"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>

            <ListView
                android:id="@+id/foodlist"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </LinearLayout>
    </ScrollView>
    <include layout="@layout/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>